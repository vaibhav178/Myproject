11A)
import java.util.Scanner;

import java.io.*;

class Main {

                static int ODConvert(int n)

                {

                                int num = n;

                                int dec = 0;

                                int base = 1;

                                int temp = num;

                                while (temp > 0) {

                                                int rem = temp % 10;

                                                temp = temp / 10;

 

                                                dec += rem * base;

 

                                                base = base * 8;

                                }

                                return dec;

                }

 

                public static void main(String[] args)

                {

                                Scanner sc = new Scanner(System.in);   

                                System.out.print("Enter an octal number: ");

                                int num = sc.nextInt();

                                System.out.println(ODConvert(num));

                }

}
