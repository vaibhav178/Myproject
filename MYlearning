7A)
import java.util.Scanner;

public class Main

{

public static void main(String[] args) {

Scanner sc = new Scanner(System.in);    

System.out.print("Enter a binary number : ");

int bin = sc.nextInt();

int dec = 0;

int n = 0;  

while(bin > 0)

{

int temp = bin%10; 

dec += temp*Math.pow(2, n);  

bin = bin/10;  

n++;  

}

System.out.println(dec); 

}

}
