10A)
 import java.util.Scanner;

public class Main

{

                public static void main(String args[])

                {     

                                Scanner sc = new Scanner(System.in);   

                                System.out.print("Enter an octal number: ");

                                int oct = sc.nextInt();

                                int dec = 0;

                                int n = 0; 

                                while(oct > 0)

                                {

                                                int temp = oct % 10; 

                                                dec += temp * Math.pow(8, n); 

                                                oct = oct/10; 

                                                n++; 

                                }

                                int bin[] = new int[20];

                                int i = 0;

                                while(dec > 0)

                                {      

                                                int r = dec % 2;

                                                bin[i++] = r;

                                                dec = dec/2;

                                }

                               

                                for(int j = i-1 ; j >= 0 ; j--)

                                System.out.print(bin[j]);

                }

} 
