9A)
import java.util.Scanner;

public class Main 

{

public static void main(String args[])

{

Scanner sc = new Scanner(System.in);    

System.out.print("Enter a binary number : ");

int bin = sc.nextInt();

int dec = 0;

int n = 0;

while(bin > 0)

{

int temp = bin%10;  

dec += temp*Math.pow(2, n);  

bin = bin/10;  

n++;  

}

int oct[] = new int[20];

int i = 0;

while(dec > 0)

{

int r = dec % 8;

oct[i++] = r;

dec = dec / 8;

}

for(int j = i-1 ; j >= 0 ; j--)

System.out.print(oct[j]); 

}

}
