4A) import java.util.Scanner;

public class Main

{

                public static void main(String[] args) {

                                Scanner sc=new Scanner(System.in);

                                System.out.print("Enter lower and upper bounds: ");

                                int n1 = sc.nextInt();

                                int n2 = sc.nextInt();

                                int count;

                                for(int i = n1 ; i <= n2 ; i++)

                                {

                                                count = 0;

                                                for(int j = 1 ; j <= i ; j++)

                                                {

                                                                if(i % j == 0)

                                                                                count = count+1;

                                                }

                                                if(count == 2)

                                                                System.out.print(i+" ");

                                }

                }

}
